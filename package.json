{
  "name": "export-all-typescript",
  "displayName": "export all typescript",
  "description": "Export all files from folder in an index file",
  "version": "1.0.1",
  "publisher": "fega",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.exportAllTypescript"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.exportAllTypescript",
        "title": "exportAllTypescript: Export all files in current folder"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.14.13",
    "mocha": "^2.3.3",
    "vscode": "^1.0.0"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "typescript": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fega/export-all-typescript.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fega/export-all-typescript/issues"
  },
  "homepage": "https://github.com/fega/export-all-typescript#readme"
}
